{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "babel-preset-vite"
  ],
  "plugins": [
    "@vue/babel-plugin-jsx",
    "babel-plugin-transform-import-meta",
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "@": "./src"
        }
      }
    ]
  ]
}