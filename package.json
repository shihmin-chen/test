{
  "name": "@asus-aics/xui",
  "version": "2.0.0-master.52",
  "type": "module",
  "description": "xHIS common ui",
  "exports": {
    "./xui.css": "./dist/index.css",
    "./assets/*": "./dist/assets/*",
    ".": {
      "import": [
        "./dist/index.mjs",
        "./index.ts"
      ],
      "require": [
        "./dist/index.js",
        "./index.ts"
      ],
      "types": [
        "./dist/index.d.ts",
        "./index.ts"
      ]
    },
    "./*": "./dist/*",
    "./package.json": "./package.json"
  },
  "files": [
    "/dist/**",
    "package.json",
    "!**/stories"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "source": "./src",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "test": "jest",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit --maxWorkers=2",
    "report": "jest --coverage",
    "vue:tsc": "vue-tsc --noEmit",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "pre-build": "./scripts/pre-build.sh",
    "build": "npm run pre-build && npm run type-check && vite build",
    "build:preview": "vite build --mode previewxui",
    "preview": "vite preview --mode previewxui",
    "ts": "vue-tsc",
    "pack": "aicsapi-frontend-server",
    "storybook": "tsx ./src/stories/utils/generateIconStories.ts && storybook dev -p 6006",
    "build-storybook": "tsx ./src/stories/utils/generateIconStories.ts && storybook build -o ./xui-storybook"
  },
  "dependencies": {
    "@kyvg/vue3-notification": "^2.6.1",
    "@popperjs/core": "^2.11.0",
    "@tiptap/core": "^2.0.3",
