    "resize-observer-polyfill": "^1.5.1",
    "sass": "1.51.0",
    "storybook": "^8.6.3",
    "tippy.js": "^6.3.7",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "unplugin-vue-markdown": "^0.26.2",
    "vite": "^5.4.7",
    "vite-plugin-dts": "^4.2.2",
